{
  "name": "rescript-react-navigation",
  "description": "ReScript bindings for react-navigation.",
  "version": "5.1.3",
  "peerDependencies": {
    "@react-navigation/bottom-tabs": "^5.0.0",
    "@react-navigation/drawer": "^5.0.0",
    "@react-navigation/material-bottom-tabs": "^5.0.0",
    "@react-navigation/material-top-tabs": "^5.0.0",
    "@react-navigation/native": "^5.0.0",
    "@react-navigation/stack": "^5.0.0",
    "@rescript/react": "^0.10.0",
    "rescript-react-native": "^0.64.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": "https://github.com/rescript-react-native/react-navigation.git",
  "license": "MIT",
  "keywords": [
    "rescript",
    "react-native",
    "react-navigation"
  ],
  "files": [
    "*.md",
    "bsconfig.json",
    "src/**/*.re",
    "src/**/*.res",
    "src/**/*.resi",
    "src/**/*.js",
    "!src/**/*.bs.js"
  ],
  "scripts": {
    "format:most": "prettier --write \"**/*.{md,json,js,css}\"",
    "format:re": "find . -name \"*.re\" -or -name \"*.rei\" | grep -v \"node_modules\" | xargs bsrefmt --in-place",
    "format": "yarn format:most && yarn format:re",
    "re:start": "rescript -w",
    "re:build": "rescript",
    "re:clean-build": "rescript clean && rescript",
    "start": "yarn re:start",
    "build": "yarn re:build",
    "test": "yarn re:clean-build",
    "release": "npmpub"
  },
  "devDependencies": {
    "@rescript/react": "^0.10.0",
    "husky": "^4.0.0",
    "lint-staged": "^10.0.0",
    "npmpub": "^5.0.0",
    "prettier": "^2.0.0",
    "rescript": "^9.1.2",
    "rescript-react-native": "^0.64.3"
  },
  "prettier": {
    "trailingComma": "all"
  },
  "lint-staged": {
    "*.{md,json,js,css}": [
      "prettier --write"
    ],
    "*.{re,rei}": [
      "bsrefmt --in-place"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
